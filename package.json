    "@types/node": "^18.15.11",
    "@types/sass": "^1.43.1",
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.7",
    "should": "^13.2.3",
    "ts-node": "^10.9.1",
    "type-fest": "^3.7.2",
    "typescript": "^5.0.3"
